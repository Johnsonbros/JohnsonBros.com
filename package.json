  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run --config vitest.client.config.ts",
    "test:integration": "vitest run --config vitest.config.ts",
    "test:server:run": "vitest run --config vitest.config.ts",
    "test:client:run": "vitest run --config vitest.client.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:a11y": "playwright test e2e/accessibility.spec.ts",
    "test:seo": "playwright test e2e/seo.spec.ts",
    "test:load": "k6 run load-tests/scenarios.js",
    "test:load:smoke": "k6 run --env SCENARIO=smoke load-tests/scenarios.js",
    "test:load:stress": "k6 run --env SCENARIO=stress load-tests/scenarios.js",
    "test:all": "npm run test && npm run test:e2e",
    "seo:validate": "npm run seo:redirects && npm run seo:schema && npm run seo:sitemap && npm run seo:perf",
    "seo:redirects": "tsx server/src/seo/validateRedirects.ts",
    "seo:schema": "tsx server/src/seo/validateSchema.ts",
    "seo:sitemap": "tsx server/src/seo/validateSitemap.ts",
    "seo:perf": "tsx server/src/seo/measurePerformance.ts",
    "seo:report": "tsx server/src/seo/generateReport.ts"
  },
